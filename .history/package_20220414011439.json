{
   "name": "class-manager-app",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "ts-node src/index.ts",
      "lint": "next lint",
      "db:setup": "npm run db:create:dev && npm run db:create:test",
      "db:create:dev": "createdb --role=nixcloud --owner=nixcloud class_manager_dev",
      "db:create:test": "createdb --role=nixcloud --owner=nixcloud class_manager_test",
      "g:migration": "npm run typeorm:local migration:generate -n",
      "g:entity": "npm run typeorm:local entity:create -n",
      "typeorm:local": "./node_modules/typeorm/cli.js",
      "local": "DOTENV_CONFIG_PATH=./.env ts-node -P ./tsconfig.json -r dotenv/config"
   },
   "dependencies": {
      "@types/pg": "^8.6.5",
      "fp-ts": "^2.11.9",
      "next": "12.1.5",
      "pg": "^8.4.0",
      "react": "18.0.0",
      "react-dom": "18.0.0",
      "react-icons": "^4.3.1",
      "reflect-metadata": "^0.1.13",
      "swr": "^1.3.0",
      "typeorm": "0.3.6",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@types/node": "^16.11.10",
      "@types/react": "18.0.3",
      "@types/react-dom": "18.0.0",
      "chance": "^1.1.8",
      "eslint": "8.13.0",
      "eslint-config-next": "12.1.5",
      "pg": "^8.7.3",
      "prisma": "^3.12.0",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}
